{
	"name": "build-the-typescript",
	"description": "test",
	"author": "Jay Whitewood <jakub.jay.olan@gmail.com>",
	"packageManager": "yarn@3.1.1",
	"version": "0.0.1",
	"main": "dist/ncc/index.js",
	"bin": "dist/tsc/index.js",
	"type": "module",
	"scripts": {
		"build:tsc": "tsc -p tsconfig.json",
		"dev": "node scripts/dev.js",
		"postinstall": "electron-builder install-app-deps",
		"build:ncc": "ncc build src/index.ts -a -s -m -o dist/ncc --stats-out dist/ncc/stats.json",
		"build:esb": "esbuild src/index.ts --bundle --minify --sourcemap --platform=node --target=node16 --outfile=dist/esb/index.js",
		"build:pkg": "yarn build:tsc && pkg . --compress Brotli -o dist/pkg/ttb -d --public-packages \"*\"",
		"build:eb": "electron-builder",
		"build": "node scripts/build.js"
	},
	"build": {
		"appId": "com.keinsell.example",
		"mac": {
			"category": "public.app-category.developer-tools"
		},
		"nodeGypRebuild": true
	},
	"pkg": {
		"scripts": [
			"dist/tsc/**/*.js"
		],
		"assets": [
			"./node_modules/**/*.node"
		],
		"targets": [
			"latest-macos-x64",
			"latest-win-x64"
		],
		"outputPath": "dist/pkg"
	},
	"devDependencies": {
		"@types/archiver": "^5",
		"@types/bcrypt": "^5",
		"@types/prettier": "^2",
		"@vercel/ncc": "^0.33.1",
		"archiver": "^5.3.0",
		"electron": "^16.0.6",
		"electron-builder": "^22.14.5",
		"esbuild": "^0.14.11",
		"execa": "^6.0.0",
		"node-gyp": "^8.4.1",
		"ora": "^6.0.1",
		"pkg": "^5.5.1",
		"prettier": "^2.5.1",
		"ts-node-dev": "^1.1.8",
		"typescript": "next"
	},
	"dependencies": {
		"bcrypt": "^5.0.1",
		"consola": "^2.15.3",
		"fastify": "^3.25.3",
		"input": "^1.0.1",
		"mongoose": "^6.1.5"
	}
}
